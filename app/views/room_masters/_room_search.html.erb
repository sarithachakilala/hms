<table id="tablesorter-demo" class="tablesorter" border="0" cellpadding="0" cellspacing="1">
<thead>
    <th>S.No</th>
	<th>Org code</th>
    <th>Org location</th>
    <th>Name</th>
    <th>Code</th>
    <th>Floor</th>
    <th>No of beds</th>
    <th>Extension no</th>
    <th>Status</th>
    <th colspan="3">Premissions</th>
  </tr>
</thead>
<tbody>
<% @i=0 %>
<% @room_masters.each do |room_master| %>
<% if((@i % 2)==0) 
		id="datacellone"
	else
		id="datacelltwo"
  end %>
	<tr class='<%= id %>'>
	<td><%= @i+=1 %></td>
	<td><%=h room_master.org_code %></td>
    <td><%=h room_master.org_location %></td>
    <td><%=h room_master.name %></td>
    <td><%=h room_master.code %></td>
    <td><%=h room_master.floor %></td>
    <td><%=h room_master.no_of_beds %></td>
    <td><%=h room_master.extension_no %></td>
    <td><%=h room_master.status %></td>
	<td  align="center"><a href="room_masters/<%= room_master.id %>?user_id=<%= @user_id %>"><image src="images/buttons/display1.png" size = "54x21" border ="0" /></a></td>
	<td  align="center"><a href="room_masters/<%= room_master.id %>/edit?user_id=<%= @user_id %>"><image src="images/buttons/modify.png" size = "54x21" border ="0" /></a></td>
	<td align="center"><%= link_to image_tag("buttons/delete.png", :class => "info", :border =>"0"), room_master, :confirm => 'Are you sure?', :method => :delete %></td>
   </tr>
<% end %>
</table>

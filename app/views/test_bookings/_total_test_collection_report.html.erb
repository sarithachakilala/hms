<br/>
<table align="center" width="50%" border="1" cellspacing=0>
	<tr bgcolor ="#378bf8">
		<th>S.No</th>
		<th>Date</th>
		<th>Amount</th>
	</tr>
<% net_amount=0 %>
<% for i in 0...@date.length %>
	<tr>
		<td><%= i+1 %></td>
		<td><%= @date[i] %></td>
		<td align="right"><%= @amount[i] %></td>
	</tr>
<% end %>
	<tr bgcolor ="yellow">
		<td colspan="2" align="right">Net Amount</td>
		<td align="right"><%= @amount.sum %></td>
	</tr>
</table>
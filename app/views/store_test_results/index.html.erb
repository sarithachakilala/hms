<h1>Listing store_test_results</h1>

<table>
  <tr>
    <th>Investigation</th>
    <th>Mr no</th>
    <th>Admn no</th>
    <th>Patient type</th>
  </tr>

<% @store_test_results.each do |store_test_result| %>
  <tr>
    <td><%=h store_test_result.investigation_id %></td>
    <td><%=h store_test_result.mr_no %></td>
    <td><%=h store_test_result.admn_no %></td>
    <td><%=h store_test_result.patient_type %></td>
    <td><%= link_to 'Show', store_test_result %></td>
    <td><%= link_to 'Edit', edit_store_test_result_path(store_test_result) %></td>
    <td><%= link_to 'Destroy', store_test_result, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New store_test_result', new_store_test_result_path %>
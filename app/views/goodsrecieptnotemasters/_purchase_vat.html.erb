<table width="80%" cellspacing=0 border=1>	<tr bgcolor ="#378bf8" style="color:white;">    <th>S.No</th>      <th>Date</th>	 <th>5% Vat Value</th>      <th>14.5% Vat Value</th>	  <th>Total Value</th>	  <th>Net Amount</th>	         </tr><% @i=0 %><% @tot=0 %>     <% @goodsrecieptnotemaster.each do |goodsrecieptnotemaster| %>     <% if((@i%2)==0) %>       <%  id="datacellone"%>     <%  else %>	 <% id="datacelltwo"%>     <% end %>         <tr class='<%= id %>'>		 <td><%= @i+=1 %></td> 		<td><%=h goodsrecieptnotemaster.invoice_date %></td>              <td><%=h goodsrecieptnotemaster.five_vat %></td>              <td><%=h goodsrecieptnotemaster.fourt_vat %></td>		<td><%=h (goodsrecieptnotemaster.five_vat)+(goodsrecieptnotemaster.fourt_vat) %></td>		<td><%=h @net=goodsrecieptnotemaster.net_invoice_amount	 %></td>	   	<% @tot=@tot+@net %>  </tr><% end %><tr>   	<br/><td colspan="5" class="value"><font color="black"><b>Total Amount </b></font></td><td class="value"><font color="black"><%= @tot %></font></td></tr> </div>
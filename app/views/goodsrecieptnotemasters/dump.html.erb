<% form_for :dump, :url=>{:controller=>"goodsrecieptnotemasters", :action=>"csv_import"}, :html => { :multipart => true } do |f| -%>
 <br/><br/>
 <table align="center">
   <tr>
     <td>
      <label for="dump_file">
        Select a CSV File :
      </label>
     </td>
     <td >
       <%= f.file_field :file -%>
     </td>
   </tr>
   <tr>
     <td colspan='2' class="button">
       <%= submit_tag 'Submit' -%>
     </td>
   </tr>
 </table>
<% end -%>
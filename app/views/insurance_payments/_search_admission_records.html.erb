<table id="tablesorter-demo" class="tablesorter" border="0" cellpadding="0" cellspacing="1">
<thead>
   <tr>
    <th>Ins.no</th>
    <th>Admn.No</th>
    <th>MR No</th>
    <th>Admn.Date</th>
    <th>Dis.Date</th>
	<th>Amount</th>
    <th>Dispatch Date</th>
    <th>Cheque issue date</th>
    <th>Cheque submit date</th>
    <th>Status</th>
    <th colspan="2">Permissions</th>
  </tr>
 </thead>
<tbody>
<% @i=0 %>
<% @insurance_payments.each do |insurance_payment| %>
  <% if((@i % 2)==0) 
		id="datacellone"
	else
		id="datacelltwo"
  end %>
  <tr class='<%= id %>'>
    <td><%=h insurance_payment.ins_no %></td>
    <td><%=h insurance_payment.admn_no %></td>
    <td><%=h insurance_payment.mr_no %></td>
    <td><%=h insurance_payment.admn_date %></td>
    <td><%=h insurance_payment.dis_date %></td>
    <td><%=h insurance_payment.dispatch %></td>
    <td><%=h insurance_payment.cheque_issue_date %></td>
    <td><%=h insurance_payment.cheque_submit_date %></td>
    <td><%=h insurance_payment.status %></td>
    <td class='abc'><a href="/insurance_payments/<%= insurance_payment.id %>?user_id=<%= @user_id %>"><image src="images/buttons/display1.png" size = "54x21" border ="0" /></a></td>
    <td class='abc'><a href="/insurance_payments/<%= insurance_payment.id %>edit?user_id=<%= @user_id %>"><image src="images/buttons/modify.png" size = "54x21" border ="0" /></a></td>
  </tr>
<% end %>
</table>
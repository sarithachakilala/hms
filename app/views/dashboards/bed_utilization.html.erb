<%= stylesheet_link_tag 'css' %>
<%= javascript_include_tag :defaults %>
<script type="text/javascript"> 
	
	var query1=""
	var selectid;
	
	
	function clear_function()
	{
	
		document.getElementById('ward').value=""
		document.getElementById('room').value=""
		document.getElementById('status').value=""
	}
	
	function check()
	
	{
	        
			var selObj = document.getElementById('ward_c');
			ward= selObj.options[selObj.selectedIndex].value
			var selObj = document.getElementById('org_code_c');
			org_code= selObj.options[selObj.selectedIndex].value
            if(ward=="" && org_code=="")			
			{
			   alert("Please select all fields")
			 }  
			   else
			   
			 document.getElementById("frame").src="/dashboards/dashboard_of_bed/1?ward="+ward+"&org_code="+org_code
	        
	}
	
	
</script> 
<body onload="document.getElementById('ward').focus();">
<div id="form_name_display">
	<table width="100%" border=0" cellspacing="0" cellpadding="0">
  	<tr>
    		<td width="50%">Dashboards >>&nbsp;&nbsp;&nbsp;<a HREF='/dashboards/bed_utilization/1' class="nav" style="text-decoration:none;">Bed Utilizations</a></td>
    		<td align="right" width="50%"><%= format_date(:short, Date.today ) %>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>
  	</tr>
	</table>
</div><br/>
<div id="form_data">
<fieldset>
<legend><div class="font"><b>Search</b></div></legend> 
<br/>
<table align="center"><tr>
<td>Org.Code</td>
<td><%= select :org_code,"c", Person.find(:all, :select =>"DISTINCT org_code").collect{|p| [p.org_code]},{:prompt=> false} %>
<td>Ward</td>
<td><%= select :ward,"c", WardMaster.find(:all, :select =>"DISTINCT name").collect{|p| [p.name]},{:prompt=> false} %>
<td><a href="javascript:void(0);" class="new1" onClick ="check()">Search</a>


</table>
</fieldset>
<br/>
 <center>
   <iframe src="" id="frame" width="90%" height="400px" align="center" style="border:none"/>
</center>

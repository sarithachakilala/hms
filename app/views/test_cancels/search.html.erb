<div id="form_name_display">
	<table width="100%" border=0" cellspacing="0" cellpadding="0">
  	<tr>
    		<td width="50%">Laboratory >> &nbsp;&nbsp;&nbsp;<a HREF='javascript:void(0);' id="store_test_result" onclick="setMyDiv('store_test_results/search/1?user_id=<%= @user_id %>','new','')" class="nav" style="text-decoration:none;">Perform Tests</a></td>
    		<td align="right" width="50%"><%= format_date(:short, Date.today ) %>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>
  	</tr>
	</table>
</div>
<div id="form_data">

<script type="text/javascript">
	function set_url()
	{
		from_date=document.getElementById('from_date').value
		to_date=document.getElementById('to_date').value
		lab_id=document.getElementById('lab_id').value
		mr_no=document.getElementById('mr_no').value
		first_name=document.getElementById('first_name').value
		document.getElementById('search_result').src="/test_cancels/search_results/1?from_date="+from_date+"&to_date="+to_date+"&lab_id="+lab_id+"&mr_no="+mr_no+"&first_name="+first_name+"&form_name=lab"
	}
</script>
<body onload="set_url()">
<% @date=format_date(:sort, Date.today ) %>
<fieldset><legend>Search Tests</legend>
	<table>
		<tr>
			<td>Form Date &nbsp;<%= text_field_tag('from_date','',:size=>20, :value => @date) %><a href="javascript:void(0)" onClick="setYears(2000, 2050);showCalender(this,'from_date');" >
			<img src="/images/cal.gif" ></a></td>
			<td>&nbsp;</td>
			<td>To Date &nbsp;<%= text_field_tag('to_date','',:size=>20, :value => @date) %><a href="javascript:void(0)" onClick="setYears(2000, 2050);showCalender(this,'to_date');" >
			<img src="/images/cal.gif" ></a></td>
			<td>&nbsp;</td>
			<td>Lab Id &nbsp;<%= text_field_tag('lab_id','',:size=>20) %></td>
			<td>&nbsp;</td>
			<td>MR NO &nbsp;<%= text_field_tag('mr_no','',:size=>20) %></td>
			<td>&nbsp;</td>
			<td>First Name &nbsp;<%= text_field_tag('first_name','',:size=>20) %></td>
			<td><a href="javascript:void(0)" onclick="set_url()"><%= image_tag("buttons/search.png",:size=>"54x21",:border =>0) %></a></td>
		</tr>
	</table>
</fieldset>
<iframe src="" id="search_result" width="100%" height="430px" frameborder=0></iframe>
<table id="calenderTable">
    <tbody id="calenderTableHead">
        <tr>
			<td colspan="4" align="center">
	          <select onChange="showCalenderBody(createCalender(document.getElementById('selectYear').value,
	           this.selectedIndex, false));" id="selectMonth">
	              <option value="0">Jan</option>
	              <option value="1">Feb</option>
	              <option value="2">Mar</option>
	              <option value="3">Apr</option>
	              <option value="4">May</option>
	              <option value="5">Jun</option>
	              <option value="6">Jul</option>
	              <option value="7">Aug</option>
	              <option value="8">Sep</option>
	              <option value="9">Oct</option>
	              <option value="10">Nov</option>
	              <option value="11">Dec</option>
	          </select>
            </td>
            <td colspan="2" align="center">
			    <select onChange="showCalenderBody(createCalender(this.value, 
				document.getElementById('selectMonth').selectedIndex, false));" id="selectYear">
				</select>
			</td>
          
		</tr>
	</tbody>
	<tbody id="calenderTableDays">
		<tr style="">
			<td>Sun</td><td>Mon</td><td>Tue</td><td>Wed</td><td>Thu</td><td>Fri</td><td>Sat</td>
		</tr>
	</tbody>
    <tbody id="calender"></tbody>
</table>
</body>

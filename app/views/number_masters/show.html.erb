		<td align="center"><%= f.text_field :mr_no, :class =>"text_field", :id => "mr_no", :onKeyup => "print_prev('mr_no','mr_no_length','mr_suffix','mr_prev')" %></td>
		<td align="center"><%= f.text_field :mr_no_length, :class =>"text_field", :id => "mr_no_length", :readOnly => true, :onKeyup => "print_prev('mr_no','mr_no_length','mr_suffix','mr_prev')" %></td>

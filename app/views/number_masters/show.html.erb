<script type="text/javascript">
	function print_prev(prefix,next_number,suffix,prev)
	{
		document.getElementById(prev).value=document.getElementById(prefix).value+""+document.getElementById(next_number).value+""+document.getElementById(suffix).value;
	}

</script>
<style type="text/css">
<!--
.allcaps {
font-variant: small-caps;
text-transform: uppercase;
}
-->
</style>

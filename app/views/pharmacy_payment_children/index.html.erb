<h1>Listing pharmacy_payment_children</h1>

<table>
  <tr>
    <th>Pharmacy payment</th>
    <th>Invoice date</th>
    <th>Invoice no</th>
    <th>Po no</th>
    <th>Amount</th>
    <th>Check option</th>
  </tr>

<% @pharmacy_payment_children.each do |pharmacy_payment_child| %>
  <tr>
    <td><%=h pharmacy_payment_child.pharmacy_payment_id %></td>
    <td><%=h pharmacy_payment_child.invoice_date %></td>
    <td><%=h pharmacy_payment_child.invoice_no %></td>
    <td><%=h pharmacy_payment_child.po_no %></td>
    <td><%=h pharmacy_payment_child.amount %></td>
    <td><%=h pharmacy_payment_child.check_option %></td>
    <td><%= link_to 'Show', pharmacy_payment_child %></td>
    <td><%= link_to 'Edit', edit_pharmacy_payment_child_path(pharmacy_payment_child) %></td>
    <td><%= link_to 'Destroy', pharmacy_payment_child, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New pharmacy_payment_child', new_pharmacy_payment_child_path %>